  "scripts": {
    "compile": "tsc -b",
    "start": "vite",
    "start-container": "vite --host",
    "build": "npm run build --workspaces --if-present && vite build",
    "test": "npm run test --workspaces --if-present && vitest",
    "lint": "npm run lint --workspaces --if-present && eslint src scripts tests *.ts --max-warnings 0 && prettier '**/*.{css,json}' --check",
    "lint-fix": "npm run lint:fix --workspaces --if-present && eslint src scripts tests *.ts --fix && prettier '**/*.{css,json}' --write",
    "generate-types": "exec scripts/generate-types.sh",
    "generate-licenses": "license-checker-rseidelsohn --json --production --direct --excludePackagesStartingWith '@types' --excludePrivatePackages --customPath src/common/NavBar/json/licenseCustomFormat.json > src/common/NavBar/json/licenses.json",
    "e2e-tests": "playwright test",
    "i18n-api-errors": "tsx ./scripts/i18n-api-errors.ts",
    "i18n-checker": "tsx ./scripts/i18n-checker.ts",
    "build-ui": "npm run build --workspace ui/ui-icons --workspace ui/ui-core --workspace ui/ui-charts --workspace ui/ui-warped-map"
  },
  "browserslist": {
    "production": [
      "> 0.2% in FI",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
